{
  "name": "ccccli",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "Contentful <opensource@contentful.com>",
  "private": true,
  "bin": {
    "ccccli": "./bin/run",
    "ccccli-dev": "./bin/dev"
  },
  "homepage": "https://github.com/marcolink/ccccli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "marcolink/ccccli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@contentful/jsondiffpatch": "^1.3.1",
    "@oclif/core": "^2.8.5",
    "@oclif/plugin-plugins": "^3.1.2",
    "axios": "^0.27.2",
    "chalk": "^5.2.0",
    "contentful": "^9.2.9",
    "contentful-collection": "^0.0.4",
    "contentful-sdk-core": "^7.1.0",
    "fast-json-patch": "^3.1.1",
    "listr2": "^6.6.0",
    "lodash": "^4.17.21",
    "p-limit": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@oclif/test": "^2.3.22",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.68",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "chai": "^4",
    "contentful-management": "^10.36.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mocha": "^9",
    "oclif": "^3",
    "ora": "^6.3.1",
    "prettier": "^2.8.8",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^5.0.4"
  },
  "oclif": {
    "bin": "ccccli",
    "dirname": "ccccli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "check-types": "tsc",
    "commitlint": "commitlint",
    "lint": "eslint src --ext '.ts,.js'",
    "lint-fix": "eslint src --ext '.ts,.js' --fix",
    "lint-staged": "lint-staged",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prettier": "prettier --config ./.prettierrc --check \"{**/*.js,**/*.ts,**/*.json}\"",
    "prettier-fix": "prettier --config ./.prettierrc --write \"{**/*.js,**/*.ts,**/*.json}\"",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "husky install",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/marcolink/ccccli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
